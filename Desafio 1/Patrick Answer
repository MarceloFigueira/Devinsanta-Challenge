package desafio.devinsanta;

/**
 *
 * @author Patrick Son
 */
public class DesafioDevInSanta {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        for (int i = 1; i <= 200; i++) {
            if (i % 5 == 0) {
                System.out.println(i + " Santa");
            }
            if (i % 6 == 0) {
                System.out.println(i + " rÃ©m");
            }
            if (i % 6 == 0 && i % 5 == 0) {
                System.out.println(i + " Santarem");
            }
        }
    }

}
